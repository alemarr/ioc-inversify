{
  "name": "ioc-inversify",
  "version": "1.0.0",
  "description": "An inversion of control POC using inversify and Typescript",
  "main": "src/app.ts",
  "scripts": {
    "start": "ts-node ./src/app.ts",
    "watch": "nodemon",
    "lint": "eslint . --ext .ts",
    "test": "jest --no-cache --detectOpenHandles --forceExit",
    "test:coverage": "jest --no-cache --detectOpenHandles --coverage --forceExit",
    "test:watch": "jest --no-cache --detectOpenHandles --coverage --watchAll --forceExit"
  },
  "dependencies": {
    "body-parser": "^1.20.0",
    "inversify": "^6.0.1",
    "inversify-express-utils": "^6.4.3",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.27",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "eslint": "^8.14.0",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "author": "Alejandro Marr",
  "license": "ISC"
}
